{
  "name": "cull-web-ui",
  "version": "1.0.0",
  "description": "UI for Cull web application in React-Redux",
  "main": "index.js",
  "scripts": {
    "start": "npm run build-dev && node --inspect-brk ./distlib/src/server/index.js",
    "build-dev": "npm run copy && tsc -p ./ && NODE_ENV=LOCAL DEBUG_MODE=TRUE webpack --config ./distlib/webpack.config.js --mode development",
    "build-prod": "npm run copy && tsc -p ./ && NODE_ENV=PROD webpack --config ./distlib/webpack.config.js --mode production",
    "copy": "copyfiles src/**/assets/* distlib/ && copyfiles src/**/config/*.json distlib/",
    "test": "NODE_ENV=LOCAL jest --coverage --collectCoverageFrom=src/**/*.{ts,tsx}"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/codius-smart-sharing.git"
  },
  "author": "Christopher Hubbard",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ChristopherHubbard/codius-smart-sharing/issues"
  },
  "homepage": "https://github.com/ChristopherHubbard/codius-smart-sharing#readme",
  "dependencies": {
    "@aws-amplify/auth": "^3.4.28",
    "@aws-amplify/core": "^3.8.20",
    "aws-amplify-react": "^4.2.29",
    "axios": "^0.21.1",
    "copy-webpack-plugin": "^8.0.0",
    "css-loader": "^5.1.3",
    "node-sass": "^5.0.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "redux-actions": "^2.6.5",
    "redux-form": "^8.3.7",
    "redux-observable": "^1.2.0",
    "redux-thunk": "^2.3.0",
    "rxjs": "^6.6.6",
    "sass-loader": "^11.0.1",
    "style-loader": "^2.0.0",
    "styled-components": "^5.2.1",
    "ts-loader": "^8.0.18",
    "typings-for-css-modules-loader": "^1.7.0",
    "url-loader": "^4.1.1",
    "webpack": "^5.27.1"
  },
  "devDependencies": {
    "@types/enzyme": "^3.10.8",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/jest": "^26.0.21",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.2",
    "@types/react-redux": "^7.1.16",
    "@types/react-router-dom": "^5.1.7",
    "@types/redux-actions": "^2.6.1",
    "@types/redux-form": "^8.3.1",
    "@types/redux-mock-store": "^1.0.2",
    "@types/sinon": "^9.0.11",
    "copyfiles": "^2.4.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "enzyme-to-json": "^3.6.1",
    "jest": "^26.6.3",
    "jest-cli": "^26.6.3",
    "jest-css-modules": "^2.1.0",
    "redux-mock-store": "^1.5.4",
    "sinon": "^9.2.4",
    "ts-jest": "^26.5.4",
    "typescript": "^4.2.3",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.11.2"
  }
}
